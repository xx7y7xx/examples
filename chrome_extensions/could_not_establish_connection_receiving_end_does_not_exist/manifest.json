{
  "manifest_version": 3,
  "name": "Example: Could not establish connection. Receiving end does not exist",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/"],
      "js": ["contentScripts.js"]
    }
  ],
  "background": {
    "service_worker": "backgroundScripts.js",
    "type": "module"
  },
  "permissions": ["tabs", "scripting"]
}
